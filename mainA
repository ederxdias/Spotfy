#push para o git da branch Alan

#Para executar e deixar com o commit padrao digite "make -f mainA"
#Para adicionar um commit diferente do padrao digite depois "m="commit aqui dentro""
#Exemplo de comando com um commit diferente "make -f pushD m="commit diferente""

BRANCH := Alan
override m += 

ifeq ($(strip $(m)),)
COMMIT := commit($(BRANCH))
else
COMMIT := $(m)
endif

all: push

push: commit
	@git push origin $(BRANCH)

commit: add
	@git commit -m "$(COMMIT)"
	
add:
	@git add .
